<M3Switch>
    id: switch
    key: root.key
    shadow: root.shadow
    active: root.active
    pos_hint: {"center_x": .5, "center_y": .4}
    icon_active: "check"
    icon_inactive: "close"
    icon_active_color: "#283520"
    icon_inactive_color: "#283520"
    thumb_color_active: "#bdcbaf"
    thumb_color_inactive: "#bdcbaf"
    track_color_active: "#282e23"
    track_color_inactive: "#282e23"
